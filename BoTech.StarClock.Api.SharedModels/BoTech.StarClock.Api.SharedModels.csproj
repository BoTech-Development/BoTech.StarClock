<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.3-alpha</Version>
        <Copyright>BoTech.dev All Rights Reserved.</Copyright>
        <PackageProjectUrl>https://github.com/BoTech-Development/BoTech.StarClock</PackageProjectUrl>
        <PackageIcon>BoTech-Avatar.png</PackageIcon>
        <Description>An Internal package which contains all data models which are transferred between the api and the mobile app.</Description>
        <PackageReleaseNotes>Fixed scale function of PreviewImage</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Avalonia" />
      <PackageReference Include="Newtonsoft.Json" />
      <PackageReference Include="SixLabors.ImageSharp" />
      <PackageReference Include="SkiaSharp" />
      <PackageReference Include="SkiaSharp.Views.Desktop.Common" />
      <PackageReference Include="System.Drawing.Common" />
    </ItemGroup>

    <ItemGroup>
      <None Update="BoTech-Avatar.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

</Project>
